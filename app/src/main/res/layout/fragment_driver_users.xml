<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.evolve.evx.Modules.AdminUsers.AdminUsersFragment">
    <!-- TODO: Update blank fragment layout -->
    <!--<TextView-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent"-->
    <!--android:text="@string/hello_blank_fragment" />-->
    <android.support.v4.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/swipe_refresh_driverusers"
        android:layout_width="match_parent"
        android:layout_marginTop="55dp"
        android:layout_height="wrap_content">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.evolve.evx.NonScrollListView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/driveruserslistview"
                android:divider="@null"></com.evolve.evx.NonScrollListView>

        </ScrollView>
    </android.support.v4.widget.SwipeRefreshLayout>
    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/floatingActionButton_addDriver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_margin="15dp"
        android:layout_gravity="bottom|right"
        android:src="@drawable/fab_add">



    </com.github.clans.fab.FloatingActionButton>

    <include
        layout="@layout/samm_toolbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</FrameLayout>

